<% if user_signed_in? %>
  <%= link_to "Profile", users_profile_path %>
  <%= image_tag "or.png" %>
  <%= link_to 'Log Out', destroy_user_session_path, :method => :delete %>
<% else %>
  <%= link_to "Sign up", new_user_registration_path, :remote => true %>
  <%= image_tag "or.png" %>
  <%= link_to "Log In", new_user_session_path, :remote => true %>
  <%= image_tag "slash.png" %>
  <%= link_to image_tag("fb.png"), omniauth_authorize_path(:user, "facebook") %>
<% end %>
